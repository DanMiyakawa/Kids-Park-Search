<%= link_to "新規問い合わせ", new_contact_path %>
<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>Title</th>
      <th>内容</th>
      <th>対応状況</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.title %></td>
        <td><%= contact.body %></td>
        <td><%= contact.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>