<%= form_with model:@contact, local:true do |f| %>
  <div class="form-group">
    <%= f.label "タイトル" %>
    <%= f.text_field :title, class: 'form-control title' %>
  </div>
  <div class="form-group">
    <%= f.label "内容" %>
    <%= f.text_area :body, class: 'form-control body' %>
  </div>
  <div class="form-group">
    <%= f.submit "送信", class: 'btn btn-success' %>
  </div>
<% end %>
<%= link_to contacts_path, class: "ml-5" do %>
  <button type="button" class="btn btn-danger">戻る</button>
<% end %>
